www.keycapvision.ml {
    root * /srv/
    file_server *
    @notStatic {
        not path /static/*
    }
    reverse_proxy @notStatic web:8000
    log {
        output file /var/log/caddy.log {
            roll_size 1gb
            roll_keep 5
            rool_keep_for 720h
        }
    }
}

keycapvision.ml {
    redir https://www.keycapvision.ml/{uri}
}